{
  "restartable": "rs",
  "ignore": [
    "*.log",
    "npm-debug.log*",
    ".DS_Store*",
    "node_modules/",
    ".npm",
    ".eslintcache",
    "users.json",
    "package-lock.json",
    "sessions/",
    "temp.*"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "."
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js",
  "events": {
    "start": "clear"
  }
}
